<h2> Sign Up </h2>

<form id="form" action="/process-signup" method="post">

    <% if (typeof feedbackMessage !== "undefined") { %>
      <p class="error"><%=feedbackMessage%></p>
    <% } %>

    <label for="email"> Email</label>
    <input  id="email" type="email" name="signupEmail"
      placeholder="irma@hurricanes.weather">
    <br>

    <label for="password"> Password </label>
    <input  id="password" type="password" name="password">
    <br>

    <button> Sign Up </button>
</form>
