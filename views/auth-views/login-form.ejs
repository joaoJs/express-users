<h2> Log In</h2>

<% if (logoutFeedback.length > 0) { %>
  <p class="success"><%= logoutFeedback %></p>
<% } %>


<form id="form" action="/process-login" method="post">

    <% if (error.length > 0) { %>
      <p class="error"><%=error%></p>
    <% } %>

    <label for="email"> Email</label>
    <input  id="email" type="email" name="loginEmail"
      placeholder="irma@hurricanes.weather">
    <br>

    <label for="password"> Password </label>
    <input  id="password" type="password" name="password">
    <br>

    <button> Log In </button>
</form>
